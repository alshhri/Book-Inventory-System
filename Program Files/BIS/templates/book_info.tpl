<table border="0" width="400" cellspacing="0" cellpadding="0">
				<tr>
					<td colspan="2" class="row2">Member<span> information</span> </td>
				</tr>
				<tr>
					<td width="200" class="row1">Book Title</td>
					<td width="200" class="row1" align="center">&nbsp;{books.title}</td>
				</tr>
				<tr>
					<td width="200" class="row1">Book ISBN</td>
					<td width="200" class="row1" align="center">&nbsp;{books.ISBN}</td>
				</tr>
				<tr>
					<td width="200" class="row1">Book Edition</td>
					<td width="200" class="row1" align="center">&nbsp;{books.edition}</td>
				</tr>
				<tr>
					<td width="200" class="row1">Book Price</td>
					<td width="200" class="row1" align="center">&nbsp;{books.price}</td>
				</tr>
				<tr>
					<td width="200" class="row1">Quantity books in male 
							stock </td>
					<td width="200" class="row1" align="center">&nbsp;{books.m_quantity}</td>
				</tr>
				<tr>
					<td width="200" class="row1" height="20">Quantity books in 
					female 
							stock </td>
					<td width="200" class="row1" align="left" height="20">
					<p align="center">&nbsp;{books.f_quantity}</td>
				</tr>
				<tr>
					<td width="200" class="row1" height="20">Quantity Total </td>
					<td width="200" class="row1" align="left" height="20">
					<p align="center">&nbsp;{books.total}</td>
				</tr>
				<tr>
					<td width="400" class="row1" colspan="2">
					<p align="center">
      <a href="javascript:window.print()">
			<img border="0" src="templates/images/print.gif"></a></td>
				</tr>
			</table>